// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// define User Model :

model User {
  id        Int    @id @map("_id")
  FirstName String 
  LastName  String
  Email     String @unique
  Password  String
}

